{
  "name": "api",
  "private": true,
  "description": "",
  "version": "0.0.1",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "build": "nest build",
    "clear": "npm run rmdist && rimraf temp && npx mikro-orm cache:clear",
    "depcheck": "npx depcheck",
    "format": "prettier --write --cache \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --cache --fix --ignore-path \"../../.eslintignore\"",
    "lint:test": "eslint \"{src,test}/**/*.ts\" --cache --fix",
    "rmdist": "rimraf -rf ../../dist/apps/api",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typecheck:app": "tsc --noEmit",
    "database:migration-create": "npx cross-env NODE_ENV=development mikro-orm migration:create",
    "seed": "npx cross-env NODE_ENV=development mikro-orm migration:fresh --seed"
  },
  "dependencies": {
    "@local/tsconfig": "*",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/nestjs": "^5.2.0",
    "@mikro-orm/postgresql": "^5.7.12",
    "@mikro-orm/reflection": "^5.7.12",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.0.6",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.1.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "install": "^0.13.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.10.36",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "ts-morph": "^19.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@mikro-orm/seeder": "^5.7.12",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.2",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/request-ip": "^0.0.38",
    "jest": "^29.5.0",
    "nestjs-spelunker": "^1.1.5",
    "pino-pretty": "^10.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.6.0",
    "typescript": "~5.1.3",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  }
}
